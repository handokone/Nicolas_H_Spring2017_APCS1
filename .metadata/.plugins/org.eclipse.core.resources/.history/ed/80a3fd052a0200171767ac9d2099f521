package textExcel;


public class SpreadsheetLocation implements Location
{
	private String location;
    @Override
    public int getRow()
    {
        // TODO Auto-generated method stub
        return 0;
    }

    @Override
    public int getCol()
    {
        int collumn = Integer.parseInt(location.substring(1));
        return collumn;
    }
    
    public SpreadsheetLocation(String cellName)
    {
    	location = cellName;
    }

}
