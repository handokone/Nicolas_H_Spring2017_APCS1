package textExcel;

public class Spreadsheet implements Grid
{
	private int rows = 20;
	private int cols = 12;
	public Spreadsheet(){
		Cell[][] arrayExcell = new EmptyCell[rows][cols];
	}
	
	private String fullGrid = "";

	public String processCommand(String command)
	{
		return command;
	}

	public int getRows()
	{
		return this.rows;
	}

	public int getCols()
	{
		return this.cols;
	}

	public Cell getCell(Location loc)
	{
		return null;
	}

	public String getGridText()
	{
		for(int i = 0; i < 20; i++){
			System.out.println(i);
		}
		for(char a = 'A'; a < 'L'; a++){
			System.out.print(a + "         |");
		}
		return "";
	}

}
