package textExcel;


public class SpreadsheetLocation implements Location
{
	private String location;

    public int getRow()
    {
        int rows = Integer.parseInt(location.substring(1));
        return rows - 1;
    }

    public int getCol()
    {
        char collumn = location.charAt(0);
        int cols = (int) collumn;
        return cols;
    }
    
    public SpreadsheetLocation(String cellName)
    {
    	this.location = cellName;
    }

}
